<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
<mapper namespace="_Generic_">
	
	<select id="queryTable" parameterClass="java.util.Map" resultClass="com.bl.core.ibatis.vo.ResultMap" remapResults="true">
    	SELECT $column$ FROM $table$ 
    	<isNotEmpty property="sqlList" prepend=" WHERE ">
	    	<include refid="sqlList"/>
    	</isNotEmpty>
        <isNotEmpty property="orderby">
        	ORDER BY $orderby$
        </isNotEmpty>
    </select>
	
</mapper>